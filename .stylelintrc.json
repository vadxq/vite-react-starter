{
  "extends": ["stylelint-config-standard", "stylelint-config-prettier"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "function",
          "if",
          "each",
          "include",
          "mixin",
          "extends",
          "tailwind",
          "screen",
          "apply",
          "layer",
          "variants"
        ]
      }
    ]
  },
  "ignoreFiles": ["dist/*.css"]
}
